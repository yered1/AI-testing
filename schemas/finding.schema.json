{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Finding",
  "type": "object",
  "required": [
    "id",
    "engagement_id",
    "severity",
    "title",
    "evidence"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "engagement_id": {
      "type": "string"
    },
    "asset_id": {
      "type": "string"
    },
    "severity": {
      "type": "string",
      "enum": [
        "Info",
        "Low",
        "Medium",
        "High",
        "Critical"
      ]
    },
    "cwe": {
      "type": "string"
    },
    "cve": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "recommendation": {
      "type": "string"
    },
    "evidence": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "trace": {
      "type": "object"
    }
  }
}
