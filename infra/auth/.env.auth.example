# Copy to .env.auth and fill values for your IdP (Okta, AzureAD, Google, etc.)
OAUTH2_PROXY_PROVIDER=oidc
OAUTH2_PROXY_CLIENT_ID=YOUR_CLIENT_ID
OAUTH2_PROXY_CLIENT_SECRET=YOUR_CLIENT_SECRET
OAUTH2_PROXY_OIDC_ISSUER_URL=https://your-issuer/.well-known/openid-configuration
OAUTH2_PROXY_COOKIE_SECRET=GENERATE_32B_BASE64==
OAUTH2_PROXY_EMAIL_DOMAINS=*
# Allow list of emails or groups (optional)
# OAUTH2_PROXY_ALLOWED_EMAILS=someone@your.com

# Map to orchestrator expected headers in nginx (dev default)
TENANT_DEFAULT=t_demo
