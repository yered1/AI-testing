# ------- Fill these with your IdP details -------
OAUTH2_PROXY_PROVIDER=oidc
OAUTH2_PROXY_CLIENT_ID=__YOUR_CLIENT_ID__
OAUTH2_PROXY_CLIENT_SECRET=__YOUR_CLIENT_SECRET__
# 32-byte base64 string; generate with: python -c 'import os,base64;print(base64.b64encode(os.urandom(32)).decode())'
OAUTH2_PROXY_COOKIE_SECRET=__BASE64_32B__
OAUTH2_PROXY_OIDC_ISSUER_URL=https://YOUR-IDP-ISSUER/.well-known/openid-configuration
OAUTH2_PROXY_REDIRECT_URL=http://localhost:8081/oauth2/callback

# Accept any email domain in dev; restrict in prod
OAUTH2_PROXY_EMAIL_DOMAINS=*
OAUTH2_PROXY_COOKIE_SECURE=false
OAUTH2_PROXY_COOKIE_SAMESITE=lax
OAUTH2_PROXY_SET_XAUTHREQUEST=true
